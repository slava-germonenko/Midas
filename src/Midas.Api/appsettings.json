{
  "Security": {
    "AccessTokens": {
      "AccessTokenTtlSeconds": 60
    },
    "RefreshTokens": {
      "RefreshTokenRandomBytesNumber": 32,
      "RefreshTokenLifeSpanMinutes": 1440
    },
    "Passwords": {
      "IterationsCount": 10000,
      "DefaultSaltLength": 32,
      "PasswordBytesNumber": 32
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "None"
    }
  },
  "AllowedHosts": "*"
}
